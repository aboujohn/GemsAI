{
  "name": "gemsai-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:memory": "set NODE_OPTIONS=--max-old-space-size=4096 && next dev",
    "build": "node scripts/validate-env.js && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run format:check && npm run type-check",
    "prepare": "husky",
    "validate-env": "node scripts/validate-env.js",
    "check-env": "node scripts/check-env-setup.js",
    "setup-env": "node -e \"require('fs').copyFileSync('env-template.txt', '.env.local'); console.log('Created .env.local file. Please update with your values.');\"",
    "translations:validate": "node scripts/translation-cli.js validate",
    "translations:missing": "node scripts/translation-cli.js missing",
    "translations:stats": "node scripts/translation-cli.js stats",
    "translations:scaffold": "node scripts/translation-cli.js scaffold",
    "test:all": "node scripts/test-all-tasks.js",
    "test:task": "node scripts/test-all-tasks.js --task",
    "test:integration": "node scripts/test-all-tasks.js --integration",
    "test:help": "node scripts/test-all-tasks.js --help"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.49.10",
    "@tailwindcss/typography": "^0.5.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^23.15.1",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-http-backend": "^3.0.2",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "next-i18next": "^15.3.1",
    "next-themes": "^0.4.6",
    "openai": "^5.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.57.0",
    "react-i18next": "^15.1.4",
    "recharts": "^2.15.3",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-rtl": "^0.9.0",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/lru-cache": "^7.10.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "chalk": "^4.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
